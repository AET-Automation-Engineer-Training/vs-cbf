<?xml version="1.0" encoding="UTF-8"?>
<scenario>
    <!--Obstacles-->
    <obstacle x1="-2.5" y1="-2.5" x2="-2.5" y2="2.5"/>
    <obstacle x1="-2.5" y1="2.5" x2="2.5" y2="2.5"/>
    <obstacle x1="2.5" y1="2.5" x2="2.5" y2="-2.5"/>
    <obstacle x1="40" y1="10" x2="40" y2="0"/>
    <!--Waypoints (incl. WaitingQueues)-->
    <waypoint id="left_down" x="-1" y="2" r="3"/>
    <waypoint id="left_mid" x="0" y="2" r="3"/>
    <waypoint id="left_up" x="1" y="2" r="3"/>
    <waypoint id="right_down" x="-1" y="-2" r="3"/>
    <waypoint id="right_mid" x="0" y="-2" r="3"/>
    <waypoint id="right_up" x="1" y="-2" r="3"/>
    <!--Attractions-->
    <!--Agents-->
    <!-- Robot -->
    <agent x="-2" y="0" n="1" dx="0" dy="0" type="2">
        <addwaypoint id="left_down"/>
        <addwaypoint id="right_down"/>
        <addwaypoint id="right_up"/>
    </agent>
    <!--AgentClusters-->
    <agent x="-1" y="2" n="1" dx="10" dy="6" type="0">
        <addwaypoint id="right_down"/>
        <addwaypoint id="left_down"/>

    </agent>
    <agent x="0" y="-2" n="0" dx="10" dy="6" type="0">
        <addwaypoint id="left_mid"/>
        <addwaypoint id="right_mid"/>

    </agent>

    <agent x="1" y="2" n="0" dx="10" dy="6" type="1">
        <addwaypoint id="right_up"/>
        <addwaypoint id="left_up"/>
        <addwaypoint id="right_down"/>
        <addwaypoint id="right_up"/>
    </agent>
    <agent x="10" y="7" n="0" dx="10" dy="6" type="1">
        <addwaypoint id="left_up"/>
        <addwaypoint id="left_up"/>
        <addwaypoint id="right_down"/>
        <addwaypoint id="right_up"/>
    </agent>
</scenario>
